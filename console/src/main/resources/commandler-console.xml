<?xml version="1.1" encoding="UTF-8"?>
<commandler xmlns="https://commandler.elypia.org"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="https://commandler.elypia.org commandler.xsd">
  <messengers>
    <messenger>
      <type>org.elypia.commandler.console.messengers.MiscMessenger</type>
      <provides>java.lang.String</provides>
      <compatible>
        <type>java.lang.Boolean</type>
        <type>java.lang.CharSequence</type>
        <type>java.lang.Character</type>
        <type>java.net.URL</type>
        <type>boolean</type>
        <type>char</type>
      </compatible>
    </messenger>
    <messenger>
      <type>org.elypia.commandler.console.messengers.NumberMessenger</type>
      <provides>java.lang.Number</provides>
      <compatible>
        <type>java.lang.Number</type>
        <type>double</type>
        <type>float</type>
        <type>long</type>
        <type>int</type>
        <type>short</type>
        <type>byte</type>
      </compatible>
    </messenger>
  </messengers>
</commandler>
